            VerticalContentAlignment="Center" Grid.Row="1"/>
        <Label 
            FontFamily="Webdings"
            FontStyle="Normal"
            FontSize="72"
            Content="c"
            HorizontalContentAlignment="Center"
            VerticalContentAlignment="Center" Grid.Row="1" Grid.Column="2"/>
